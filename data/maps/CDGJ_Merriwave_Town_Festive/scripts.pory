        VAR_TEMP_0, 1: CDGJ_Merriwave_Town_Festive_ResetPlayerState
        VAR_TEMP_1, 1: CDGJ_Merriwave_Town_Festive_ResetPlayerState
    ]
}

script CDGJ_Merriwave_Town_Festive_ResetPlayerState {
